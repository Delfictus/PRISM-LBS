{
  "dashboard": {
    "title": "PRISM GPU Performance",
    "description": "Comprehensive monitoring dashboard for PRISM v2 GPU-accelerated graph coloring pipeline",
    "tags": ["prism", "gpu", "rl", "neuromorphic"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "GPU Utilization",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "prism_gpu_utilization",
            "legendFormat": "GPU {{device}}",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": { "mode": "multi", "sort": "none" },
          "legend": { "displayMode": "list", "placement": "bottom" }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "showPoints": "never"
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.7, "color": "yellow" },
                { "value": 0.9, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "GPU Memory Usage",
        "type": "gauge",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "prism_gpu_memory_used_bytes / prism_gpu_memory_total_bytes",
            "legendFormat": "GPU {{device}}",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.6, "color": "yellow" },
                { "value": 0.85, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Current Phase",
        "type": "stat",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "max(prism_phase_iteration_total) by (phase)",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "blue" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Best Chromatic Number",
        "type": "stat",
        "gridPos": { "x": 18, "y": 4, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "prism_phase_chromatic_best",
            "refId": "A"
          }
        ],
        "options": {
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "decimals": 0
          }
        }
      },
      {
        "id": 5,
        "title": "Phase Progress (Duration Heatmap)",
        "type": "heatmap",
        "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 },
        "targets": [
          {
            "expr": "rate(prism_phase_duration_seconds_sum[1m])",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellRadius": 4,
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "min": 0,
            "mode": "scheme",
            "reverse": false,
            "scheme": "Spectral",
            "steps": 64
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false
          }
        }
      },
      {
        "id": 6,
        "title": "Best Chromatic Number Over Time",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "prism_phase_chromatic_best",
            "legendFormat": "Chromatic Number",
            "refId": "A"
          },
          {
            "expr": "prism_phase_conflicts",
            "legendFormat": "Conflicts",
            "refId": "B"
          }
        ],
        "options": {
          "tooltip": { "mode": "multi", "sort": "none" },
          "legend": { "displayMode": "list", "placement": "bottom" }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "showPoints": "auto"
            },
            "decimals": 0
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Chromatic Number" },
              "properties": [
                { "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Conflicts" },
              "properties": [
                { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
              ]
            }
          ]
        }
      },
      {
        "id": 7,
        "title": "RL Reward Signal (Multi-Phase)",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "prism_rl_reward",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "lineWidth": 1,
              "fillOpacity": 15,
              "gradientMode": "hue",
              "showPoints": "never"
            },
            "min": -1,
            "max": 1,
            "decimals": 3
          }
        }
      },
      {
        "id": 8,
        "title": "Kernel Performance (Average Duration)",
        "type": "barchart",
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(prism_gpu_kernel_duration_seconds_sum[5m]) / rate(prism_gpu_kernel_launches_total[5m])",
            "legendFormat": "{{kernel}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": 0,
          "xTickLabelSpacing": 100,
          "showValue": "auto",
          "stacking": "none",
          "groupWidth": 0.7,
          "barWidth": 0.97,
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": []
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "s",
            "decimals": 6,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.1, "color": "yellow" },
                { "value": 1.0, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Kernel Launch Count",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(prism_gpu_kernel_launches_total[1m])",
            "legendFormat": "{{kernel}}",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "table", "placement": "right", "calcs": ["last", "max"] }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "showPoints": "never",
              "stacking": { "mode": "none" }
            },
            "unit": "ops",
            "decimals": 2
          }
        }
      },
      {
        "id": 10,
        "title": "Phase Temperature (Thermodynamic Search)",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "prism_phase_temperature",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": { "mode": "multi", "sort": "none" },
          "legend": { "displayMode": "list", "placement": "bottom" }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "continuous-RdYlGr" },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 25,
              "gradientMode": "scheme",
              "showPoints": "never"
            },
            "min": 0,
            "decimals": 3
          }
        }
      },
      {
        "id": 11,
        "title": "Compaction Ratio (Phase 2)",
        "type": "gauge",
        "gridPos": { "x": 12, "y": 32, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "prism_phase_compaction_ratio{phase=\"Phase2\"}",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.5, "color": "yellow" },
                { "value": 0.75, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "RL Epsilon (Exploration Rate)",
        "type": "timeseries",
        "gridPos": { "x": 18, "y": 32, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "prism_rl_epsilon",
            "legendFormat": "Epsilon",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": { "mode": "single", "sort": "none" },
          "legend": { "displayMode": "list", "placement": "bottom" }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 50,
              "gradientMode": "opacity",
              "showPoints": "never"
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "blue" },
                { "value": 0.1, "color": "green" },
                { "value": 0.5, "color": "yellow" }
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "RL Action Distribution",
        "type": "piechart",
        "gridPos": { "x": 0, "y": 40, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(prism_rl_action_count[5m])) by (action)",
            "legendFormat": "{{action}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "displayLabels": ["percent"],
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["sum"]
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 14,
        "title": "Pipeline Solutions Found",
        "type": "stat",
        "gridPos": { "x": 12, "y": 40, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "prism_pipeline_solutions_found",
            "refId": "A"
          }
        ],
        "options": {
          "textMode": "value",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "decimals": 0
          }
        }
      },
      {
        "id": 15,
        "title": "Pipeline Runtime",
        "type": "stat",
        "gridPos": { "x": 18, "y": 40, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "prism_pipeline_runtime_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "unit": "s",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 60, "color": "yellow" },
                { "value": 300, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 16,
        "title": "Solution Quality Trend",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 44, "w": 12, "h": 4 },
        "targets": [
          {
            "expr": "prism_pipeline_best_chromatic",
            "legendFormat": "Best Chromatic",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": { "mode": "single", "sort": "none" },
          "legend": { "displayMode": "list", "placement": "bottom" }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "continuous-BlPu" },
            "custom": {
              "lineWidth": 3,
              "fillOpacity": 30,
              "gradientMode": "scheme",
              "showPoints": "auto"
            },
            "decimals": 0
          }
        }
      }
    ]
  }
}
